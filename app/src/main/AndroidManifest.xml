<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.biniyamshopping.shop">

    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.CALL_PHONE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_WIFI_STATE" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/shoplogo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/shoplogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <meta-data
            android:name="com.google.android.gms.ads.APPLICATION_ID"
            android:value="ca-app-pub-6002206915132015~3669917621" />
        <activity android:name=".abut" />
        <activity android:name=".Call" />
        <activity android:name=".About" />
        <activity android:name=".Home" />
        <activity android:name=".Search" />
        <activity android:name=".Display_Retrive" />
        <activity android:name=".Retrive" />
        <activity
            android:name=".Catagories"
            android:parentActivityName=".Where" />
        <activity
            android:name=".Sell"
            android:parentActivityName=".Catagories" />
        <activity android:name=".MainActivity" />
        <activity
            android:name=".Where"
            android:parentActivityName=".MainActivity" />
        <activity android:name=".Splash">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>